{
  "title": "WUnderground .NET Core Library",
  "copyright": "ThisIsWhyTheInternetExists",
  "description": "A .NET Core library for weather data retrieval from the WUnderground API",
  "version": "2.0.2-*",
  "buildOptions": {
    "emitEntryPoint": false
  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": "dnxcore50",
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.1"
        },
        "ServiceStack.Client.Core": "1.0.23",
        "ServiceStack.Text.Core": "1.0.23"
      }
    },
    "net45": {
      "dependencies": {
        "ServiceStack.Client": "4.5.4",
        "ServiceStack.Text": "4.5.4"
      },
      "frameworkAssemblies": {
        "System.Xml": "4.0.0.0"
      }
    }
  }
}
